{
  "name": "BBDUTlog",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "bbd",
  "homepage_url": "http://192.168.2.240:8080/main/fixed.jsp",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["src/background.js"],
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "webRequest",
    "http://it.bbdservice.com:8988/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://it.bbdservice.com:8988/*"],
      "js": ["src/lib/util.js", "src/contentScript.js"],
      "run_at": "document_end"
    }
  ]
}
